<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StudentTracker.Views.AppSettings">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Clear Preferences" x:Name="ClearSettings" Clicked="ClearSettings_Clicked"/>
            <Button Text="Load Sample Data" x:Name="LoadSampleData" Clicked="LoadSampleData_Clicked" />
            <Button Text="Clear Sample Data" x:Name="ClearSampleData" Clicked="ClearSampleData_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>